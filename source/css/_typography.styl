@font-face {
    font-family: 'american_purposeregular';
    src: url('fonts/american_purpose-webfont.eot');
    src: url('fonts/american_purpose-webfont.eot?#iefix') format('embedded-opentype'),
         url('fonts/american_purpose-webfont.woff2') format('woff2'),
         url('fonts/american_purpose-webfont.woff') format('woff'),
         url('fonts/american_purpose-webfont.ttf') format('truetype'),
         url('fonts/american_purpose-webfont.svg#american_purposeregular') format('svg');
    font-weight: normal;
    font-style: normal;

}

h1,h2,h3,h4,h5
  font-family 'american_purposeregular', serif
  
h3
  font-size 25px
